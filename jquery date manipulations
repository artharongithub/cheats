var dateAr = '11/12/2018'.split('/');
var newDate = dateAr[0] + '-' + dateAr[1] + '-' + dateAr[2];

const monthNames = ["Հունվար", "Փետրվար", "Մարտ", "Ապրիլ", "Մայիս", "Հունիս",
  "Հուլիս", "Օգոստոս", "Սեպտեմբեր", "Հոկտեմբեր", "Նոյեմբեր", "Դեկտեմբեր"
];
mon = (dateAr[1]-1);
const d = new Date();
